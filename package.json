{
  "name": "service-car",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky",
    "env:local": "env-cmd -f ./.env.development.local",
    "analyze-bundle": "cross-env ANALYZE=true npm run build",
    "dev": "next dev",
    "build": "next build",
    "build:local": "npm run env:local npm run build",
    "start": "next start",
    "start:local": "npm run env:local npm run start",
    "lint": "eslint . --max-warnings 0",
    "lint-staged": "lint-staged",
    "test:jest": "jest",
    "test:playwright": "playwright test",
    "test:playwright:local": "npm run env:local npm run test:playwright",
    "test:playwright:ui": "playwright test --ui",
    "test:playwright:ui:local": "npm run env:local npm run test:playwright:ui",
    "db:types:local": "supabase gen types typescript --local > types/supabase.ts",
    "db:types:production": "cross-env env-cmd -f ./.env.production.local supabase gen types typescript --project-id=$SUPABASE_PROJECT_ID --schema public > types/supabase.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "5.0.1",
    "@next/bundle-analyzer": "15.3.1",
    "@supabase/ssr": "0.6.1",
    "@supabase/supabase-js": "2.49.4",
    "@tanstack/react-query": "5.74.4",
    "motion": "12.9.4",
    "next": "15.3.1",
    "next-themes": "0.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "7.56.2",
    "tailwind-merge": "3.2.0",
    "zod": "3.24.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.25.1",
    "@playwright/test": "1.52.0",
    "@tailwindcss/postcss": "4.1.4",
    "@tanstack/eslint-plugin-query": "5.73.3",
    "@tanstack/react-query-devtools": "5.74.4",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/jest": "29.5.14",
    "@types/node": "22.14.1",
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.2",
    "cross-env": "7.0.3",
    "env-cmd": "10.1.0",
    "eslint": "9.25.1",
    "eslint-config-next": "15.3.1",
    "eslint-config-prettier": "10.1.2",
    "eslint-import-resolver-typescript": "4.3.4",
    "eslint-plugin-import-x": "4.10.6",
    "eslint-plugin-jest-dom": "5.5.0",
    "eslint-plugin-playwright": "2.2.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-testing-library": "7.1.1",
    "glob": "11.0.2",
    "globals": "16.0.0",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "lint-staged": "15.5.1",
    "postcss": "8.5.3",
    "prettier": "3.5.3",
    "prettier-plugin-tailwindcss": "0.6.11",
    "supabase": "2.22.6",
    "tailwindcss": "4.1.5",
    "ts-node": "10.9.2",
    "typescript": "5.8.3",
    "typescript-eslint": "8.31.0",
    "whatwg-fetch": "3.6.20"
  }
}
