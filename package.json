{
  "name": "service-car",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "env:test": "env-cmd -f ./.env.test.local",
    "dev": "next dev",
    "build": "next build",
    "build:test": "npm run env:test next build",
    "postbuild": "node ./scripts/removeInlineScripts.mjs",
    "postbuild:test": "node ./scripts/removeInlineScripts.mjs",
    "start": "next start",
    "start:test": "npm run env:test next start",
    "lint": "next lint",
    "prepare": "husky install",
    "test:jest": "jest",
    "test:playwright": "playwright test",
    "test:playwright:local": "npm run env:test npm run test:playwright",
    "test:playwright:headed": "playwright test --headed",
    "test:playwright:headed:local": "npm run env:test npm run test:playwright:headed",
    "test:playwright:ui": "playwright test --ui",
    "test:playwright:ui:local": "npm run env:test npm run test:playwright:ui",
    "test:playwright:report": "playwright show-report",
    "db:types:local": "supabase gen types typescript --local > types/supabase.ts",
    "db:types:production": "cross-env env-cmd -f ./.env.production.local supabase gen types typescript --project-id=$SUPABASE_PROJECT_ID --schema public > types/supabase.ts"
  },
  "dependencies": {
    "@supabase/ssr": "0.5.2",
    "@supabase/supabase-js": "2.48.1",
    "framer-motion": "11.0.5",
    "next": "14.2.15",
    "next-themes": "0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.49.3",
    "tailwind-merge": "2.1.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "14.0.4",
    "@playwright/test": "1.48.2",
    "@tailwindcss/postcss": "4.0.0",
    "@testing-library/jest-dom": "6.3.0",
    "@testing-library/react": "14.1.2",
    "@testing-library/user-event": "14.5.2",
    "@types/jest": "29.5.11",
    "@types/node": "20.10.4",
    "@types/react": "18.2.43",
    "@types/react-dom": "18.2.17",
    "@typescript-eslint/eslint-plugin": "6.14.0",
    "@typescript-eslint/parser": "6.14.0",
    "cross-env": "7.0.3",
    "env-cmd": "10.1.0",
    "eslint": "8.55.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jest-dom": "5.1.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-playwright": "2.0.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-testing-library": "6.2.0",
    "glob": "10.3.10",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "postcss": "8.5.1",
    "prettier": "3.1.1",
    "prettier-plugin-tailwindcss": "0.6.11",
    "supabase": "2.9.6",
    "tailwindcss": "4.0.0",
    "ts-node": "10.9.2",
    "typescript": "5.3.3",
    "whatwg-fetch": "3.6.20"
  }
}
