{
  "name": "service-car",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "env:local": "env-cmd -f ./.env.development.local",
    "dev": "cross-env DEVELOPMENT=true next dev",
    "build": "next build",
    "build:local": "npm run env:local npm run build",
    "postbuild": "node ./scripts/removeInlineScripts.mjs",
    "postbuild:local": "node ./scripts/removeInlineScripts.mjs",
    "start": "next start",
    "start:local": "npm run env:local npm run start",
    "lint": "eslint . --max-warnings 0",
    "test:jest": "jest",
    "test:playwright": "playwright test",
    "test:playwright:local": "npm run env:local npm run test:playwright",
    "test:playwright:ui": "playwright test --ui",
    "test:playwright:ui:local": "npm run env:local npm run test:playwright:ui",
    "db:types:local": "supabase gen types typescript --local > types/supabase.ts",
    "db:types:production": "cross-env env-cmd -f ./.env.production.local supabase gen types typescript --project-id=$SUPABASE_PROJECT_ID --schema public > types/supabase.ts"
  },
  "dependencies": {
    "@supabase/ssr": "0.5.2",
    "@supabase/supabase-js": "2.48.1",
    "framer-motion": "11.0.5",
    "next": "14.2.15",
    "next-themes": "0.4.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.54.2",
    "tailwind-merge": "3.0.1",
    "zod": "3.24.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "9.19.0",
    "@playwright/test": "1.48.2",
    "@tailwindcss/postcss": "4.0.1",
    "@testing-library/jest-dom": "6.3.0",
    "@testing-library/react": "14.1.2",
    "@testing-library/user-event": "14.5.2",
    "@types/jest": "29.5.11",
    "@types/node": "20.10.4",
    "@types/react": "18.2.43",
    "@types/react-dom": "18.2.17",
    "cross-env": "7.0.3",
    "env-cmd": "10.1.0",
    "eslint": "9.19.0",
    "eslint-config-next": "15.1.6",
    "eslint-config-prettier": "10.0.1",
    "eslint-import-resolver-typescript": "3.7.0",
    "eslint-plugin-import-x": "4.6.1",
    "eslint-plugin-jest-dom": "5.5.0",
    "eslint-plugin-playwright": "2.2.0",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-testing-library": "7.1.1",
    "glob": "10.3.10",
    "globals": "15.14.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "postcss": "8.5.1",
    "prettier": "3.4.2",
    "prettier-plugin-tailwindcss": "0.6.11",
    "supabase": "2.9.6",
    "tailwindcss": "4.0.3",
    "ts-node": "10.9.2",
    "typescript": "5.7.3",
    "typescript-eslint": "8.23.0",
    "whatwg-fetch": "3.6.20"
  }
}
